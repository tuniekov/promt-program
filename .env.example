# Порт для запуска сервера
PORT=3000

# API ключ для OpenRouter.ai
# Получите свой ключ на https://openrouter.ai/keys
OPENROUTER_API_KEY=your_api_key_here

# Активация платных моделей (true/false)
# Если установлено в false, платные модели будут видны, но недоступны для использования
ENABLE_PAID_MODELS=false
